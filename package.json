{
  "name": "oxygen-bnb",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "types": "workspace:*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "next": "12.3.1"
  },
  "packageManager": "pnpm@7.13.2"
}